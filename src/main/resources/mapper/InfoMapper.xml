<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.study.dao.InfoMapper">

    <sql id="tableName">t_info</sql>


    <select id="getById" parameterType="string" resultType="info">
        select * from
        <include refid="tableName"/>
        where id=#{id}
    </select>

</mapper>